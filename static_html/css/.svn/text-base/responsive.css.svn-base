	#getbtn{
		top:860px;
	}
	#videobox{
		max-width:400px;
		margin:0 0 0 -100px;
	}
	/* Direction Nav */
	#mainAnimate .flex-direction-nav {*height: 0; display:block;}
	#mainAnimate .flex-direction-nav li{
		position:inherit !important; width:auto; height:auto;
	}
	.flex-direction-nav a  { display: block; width: 40px; height: 40px; margin: -20px 0 0; position: absolute; top: 80%; z-index: 10; overflow: hidden; opacity: 0; cursor: pointer; color: rgba(255,255,255,0.2); -webkit-transition: none !important; -moz-transition: none !important; transition: none !important; z-index:100000; line-height:40px; font-size:20px; }
	.flex-direction-nav .flex-prev { margin:0 0 0 -20px; }
	.flex-direction-nav .flex-next { margin:0 -20px 0 0; }
	.flexslider:hover .flex-prev { opacity: 0.7; left: 10px; }
	.flexslider:hover .flex-next { opacity: 0.7; right: 10px; }
	.flexslider:hover .flex-next:hover, .flexslider:hover .flex-prev:hover { opacity: 1; }
	.flex-direction-nav .flex-disabled { opacity: 0!important; filter:alpha(opacity=0); cursor: default; }
	.flex-direction-nav a:before  { font-family: "flexslider-icon"; font-size: 20px; display: inline-block; content: '\f001'; }
	.flex-direction-nav a.flex-next:before  { content: '\f002'; }
} 

@media only screen and (max-width: 640px) {
